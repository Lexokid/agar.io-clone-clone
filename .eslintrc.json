{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "func-names":0,
        "no-console":0,
        "no-plusplus":0,
        "no-bitwise":0,
        "no-mixed-operators":0,
        "no-param-reassign":0,
        "class-methods-use-this":0,
        "prefer-destructuring":0,
        "no-shadow":0,
        "no-multi-assign":0,
        "no-use-before-define":0
    }
}
